<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication</title>

    <environment names="Development">
        <link href="~/backend/css/bootstrap.min.css" rel="stylesheet">
        <link href="~/backend/css/bootstrap-reset.css" rel="stylesheet">
        <link href="~/backend/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/backend/css/slidebars.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" />
        <link href="~/backend/css/style.css" rel="stylesheet">
        <link href="~/backend/css/style-responsive.css" rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href="~/backend/css/mixins.css">
        <link rel="stylesheet" type="text/css" href="~/backend/css/global.css">
        <link rel="stylesheet" type="text/css" href="~/backend/css/mystyle.css">
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" asp-append-version="true" />
        <link href="~/backend/css/slidebars.css" rel="stylesheet" asp-append-version="true">
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" />
        <link href="~/backend/css/style.css" rel="stylesheet">
        <link href="~/backend/css/style-responsive.css" rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href="~/backend/css/mixins.css">
        <link rel="stylesheet" type="text/css" href="~/backend/css/global.css">
        <link rel="stylesheet" type="text/css" href="~/backend/css/mystyle.css">
    </environment>
</head>
<body>

    <!--header start-->
    <header class="header white-bg">
        <div class="sidebar-toggle-box">
            <i class="fa fa-bars"></i>
        </div>
        <!--logo start-->
        <a href="#" class="logo "><img class="my_main_logo" src="~/backend/img/backlogo.png" style="max-width: 165px"></a>
        <!--logo end-->

        <div class="top-nav ">
            <ul class="nav pull-right top-menu">
                
                <!-- user login dropdown start-->
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                        <img alt="" src="~/backend/img/user.png">
                        <span class="username">Super Administrator</span>
                        <b class="caret"></b>
                    </a>


                    <ul class="dropdown-menu extended logout">
                        <div class="log-arrow-up"></div>
                        @Html.Partial("_LoginAdmin")

                    </ul>
                </li>
                <!-- user login dropdown end -->

            </ul>
        </div>
    </header>
    <!--header end-->



    <!--sidebar start-->
    <aside class="">
        <div id="sidebar" class="nav-collapse">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">

                <!--dashboard menu starts-->
                <li>
                    <a href="" class="active">
                        <i class="fa fa-dashboard"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
                <!--dashboard menu ends-->
                <!--user menu starts-->
                <li class="sub-menu">
                    <a class="" asp-area="Admin" asp-controller="Users" asp-action="Index">
                        <i class="fa fa-user"></i>
                        <span>Users</span>
                    </a>

                </li>
                <!--user menu ends-->
                <!--Roles and permissions starts-->
                <li class="sub-menu">
                    <a class="" asp-area="Admin" asp-controller="Roles" asp-action="Index">
                        <i class="fa fa-lock"></i>
                        <span>Roles </span>
                    </a>


                </li>
                <!--Roles and permissions ends-->


                <li class="sub-menu">
                    <a class="" asp-area="Admin" asp-controller="News" asp-action="Index">
                        <i class="fa fa-bell"></i>
                        <span>News </span>
                    </a>

                </li>

                <li class="sub-menu">
                    <a class="" asp-area="Admin" asp-controller="Careers" asp-action="Index">
                        <i class="fa fa-star"></i>
                        <span>Careers </span>
                    </a>

                </li>

            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->

    <section id="main-content">

        <section class="wrapper site-min-height">


            @RenderSection("Breadcrumbs", required: false)
            @RenderBody()


        </section>
    </section>




    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/stock.js" asp-append-version="true"></script>
        <script class="include" type="text/javascript" src="~/backend/js/jquery.dcjqaccordion.2.7.js"></script>
        <script src="~/backend/js/jquery.scrollTo.min.js"></script>
        <script src="~/backend/js/jquery.nicescroll.js" type="text/javascript"></script>
        <script src="~/backend/js/slidebars.min.js"></script>
        <script src="~/backend/js/respond.min.js"></script>
        <script src="~/backend/js/bootstrap-validator.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.steps.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.validate.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.stepy.js"></script>
        <script type="text/javascript" src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
        <script src="~/backend/js/common-scripts.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script class="include" type="text/javascript" src="~/backend/js/jquery.dcjqaccordion.2.7.js"></script>
        <script src="~/backend/js/jquery.scrollTo.min.js"></script>
        <script src="~/backend/js/jquery.nicescroll.js" type="text/javascript"></script>
        <script src="~/backend/js/slidebars.min.js"></script>
        <script src="~/backend/js/respond.min.js"></script>
        <script src="~/backend/js/bootstrap-validator.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.steps.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.validate.min.js" type="text/javascript"></script>
        <script src="~/backend/js/jquery.stepy.js"></script>
        <script type="text/javascript" src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
        <script src="~/backend/js/common-scripts.js"></script>
    </environment>

    @RenderSection("scripts", required: false)

</body>
</html>
