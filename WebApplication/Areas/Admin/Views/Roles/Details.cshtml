
@model WebApplication.Identity.IdentityRole
@{
    ViewData["Title"] = "Details";
}




<div class="panel">
    <header class="panel-heading" style="margin-bottom: 30px;">Details</header>
    <div class="panel-body">
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="m-0 p-t-30">
                 <div class="row">
                     <div class="col-lg-8">
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.Name)
                            </dt>
                            <dd>
                                @Html.DisplayFor(model => model.Name)
                            </dd>
                        </dl>
                    </div>
                    <div class="col-lg-4">
                        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-default">Edit</a>
                        <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
                    </div>
                 </div>
            </div>
        </div>
    </div>
</div>




@section Breadcrumbs {
    <ul class="breadcrumb">
    <li><a asp-controller="Dashboard" asp-action="Index"><i class="fa fa-home"></i> Home</a></li>
    <li><a asp-controller="Roles" asp-action="Index">Roles</a></li>                          
    <li class="active ng-binding"><a href="javascript:;">Details</a></li>
</ul>

}

