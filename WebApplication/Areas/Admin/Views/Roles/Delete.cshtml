@model WebApplication.Identity.IdentityRole
@{
    ViewData["Title"] = "Delete";
}



<div class="panel">
    <header class="panel-heading" style="margin-bottom: 30px;">Are you sure you want to delete this?</header>
    <div class="panel-body">
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="m-0 p-t-30">
                 <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Name)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>
                </dl>
                <form asp-action="Delete">
                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-danger" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>



@section Breadcrumbs {
     <ul class="breadcrumb">
    <li><a asp-controller="Dashboard" asp-action="Index"><i class="fa fa-home"></i> Home</a></li>
    <li><a asp-controller="Roles" asp-action="Index">Roles</a></li>                          
    <li class="active ng-binding"><a href="javascript:;">Delete</a></li>
</ul>

}