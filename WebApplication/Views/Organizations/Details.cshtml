@model WebApplication.Core.Domains.Organization


@{
    ViewData["Title"] = "Details";
}

<div class="panel mb25">
    <div class="panel-heading border">
        Details
    </div>
    <div class="panel-body">
        <div class="row no-margin">
            <div class="col-lg-8">
                <dl class="dl-horizontal">
              
                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>
              
                </dl>
            </div>
            <div class="col-lg-4">
                <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-default">Edit</a>
                <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
            </div>
        </div>
    </div>
</div>

@section Breadcrumbs {
    <ol class="breadcrumb">
        <li>
            <a asp-controller="Dashboard" asp-action="Index">Home</a>
        </li>
        <li>
            <a asp-controller="MessageTemplate" asp-action="Index">News</a>
        </li>
        <li class="active">
            <a href="javascript:;">Details</a>
        </li>
    </ol>
}